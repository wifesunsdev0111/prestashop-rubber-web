<?xml version="1.0" encoding="UTF-8" ?>
<themeeditor>
	<version value="1.4">
	<ccc available="true" />
	<guest_checkout available="true" />
	<one_page_checkout available="true" />
	<store_locator available="true" />
	</version>
	<author>leotheme</author>
<name>Live Theme Editor</name>	
<selectors>
	<items>
		   <header>Body Content</header>
		   <match>body</match>

		  <item>
			<type>image</type>
			<label>Background Image</label>
			<selector>body</selector>
			<attrs>background-image</attrs>
		 </item> 

		 <item>
			<type>text</type>
			<label>Background Content</label>
			<selector>body #page</selector>
			<attrs>background-color</attrs>
		</item> 
			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>body #page</selector>
				<attrs>background-image</attrs>
			</item> 
		<item>
			<type>text</type>
				<label>Container Background</label>
				<selector>.container</selector>
				<attrs>background-color</attrs>
		</item> 
	 	<item>
			<type>fontsize</type>
			<label>Font-Size</label>
			<selector>body #page</selector>
			<attrs>font-size</attrs>
		</item> 


		<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>body #page</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Link Color</label>
			<selector>a</selector>
			<attrs>color</attrs>
		</item> 

		<item>
			<type>text</type>
			<label>Link Hover Color</label>
			<selector>a:hover</selector>
			<attrs>color</attrs>
		</item>
	</items>
	<!-- top bar -->
	<items>
		<header>TopBar</header>
		<match>topbar</match>
		<item>
			<type>text</type>
			<label>Bg Color</label>
			<selector>#topbar .container</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Border</label>
			<selector>#topbar .container</selector>
			<attrs>border-color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#topbar .container</selector>
			<attrs>color</attrs>
		</item> 

	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#topbar a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>#topbar a:hover, header .nav a:hover, header .nav a.active</selector>
			<attrs>color</attrs>
		</item> 

	</items>
	<items>
		<header>Header Main</header>
		<match>header-main</match>
		<item>
			<type>text</type>
			<label>Bg Color</label>
			<selector>#header-main .container</selector>
			<attrs>background-color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#header-main .container</selector>
			<attrs>color</attrs>
		</item> 

	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#header-main a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>#header-main a:hover</selector>
			<attrs>color</attrs>
		</item> 

	</items>
	<items>
		<header>MainNav</header>
		<match>leo-mainnav</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#leo-mainnav .leo-megamenu</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>#leo-mainnav</selector>
			<attrs>border-color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#leo-mainnav a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>#leo-top-menu .dropdown-menu a:hover</selector>
			<attrs>color</attrs>
		</item> 

	</items>

	<items>
		<header>Footer Top</header>
		<match>footer-top</match>
			<item>
				<type>text</type>
				<label>Bg Color</label>
				<selector>.footer-top .inner</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Text</label>
				<selector>.footer-top .inner</selector>
				<attrs>color</attrs>
			</item> 

			<item>
				<type>text</type>
				<label>Color Link</label>
				<selector>.footer-top a</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Link Hover</label>
				<selector>.footer-top a:hover</selector>
				<attrs>color</attrs>
			</item> 

			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>.footer-top </selector>
				<attrs>background-image</attrs>
			</item>
	</items>
	<items>
		<header>Footer Center</header>
		<match>footer-center</match>
			<item>
				<type>text</type>
				<label>Bg Color</label>
				<selector>.footer-center .inner</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Text</label>
				<selector>.footer-center .inner</selector>
				<attrs>color</attrs>
			</item> 

			<item>
				<type>text</type>
				<label>Color Link</label>
				<selector>.footer-center a</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Link Hover</label>
				<selector>.footer-center a:hover</selector>
				<attrs>color</attrs>
			</item> 
			
			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>.footer-center </selector>
				<attrs>background-image</attrs>
			</item>
	</items>
	<items>
		<header>Footer Bottom</header>
		<match>footer-bottom</match>
			<item>
				<type>text</type>
				<label>Bg Color</label>
				<selector>.footer-bottom .inner</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Text</label>
				<selector>.footer-bottom .inner</selector>
				<attrs>color</attrs>
			</item> 

			<item>
				<type>text</type>
				<label>Color Link</label>
				<selector>.footer-bottom a</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Link Hover</label>
				<selector>footer-bottom a:hover</selector>
				<attrs>color</attrs>
			</item> 
			
			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>.footer-bottom </selector>
				<attrs>background-image</attrs>
			</item>
	</items>
	<items>
		<header>Powered</header>
		<match>powered</match>
		<item>
			<type>text</type>
			<label>Bg Color</label>
			<selector>.footer-nav .inner</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>.footer-nav .inner</selector>
			<attrs>color</attrs>
		</item> 

	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>.footer-nav .container a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector>.footer-nav .container a:hover</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>image</type>
			<label>Background Image</label>
			<selector>.footer-nav .container > div.inner</selector>
			<attrs>background-image</attrs>
		</item> 

	</items>
</selectors>

<elements>
	<items>
		<item>
			<type>text</type>
			<label>Color Name Product</label>
			<selector>.pb-center-column h1 , .name .product-name</selector>
			<attrs>color</attrs>
		</item> 
		<header>Products</header>
		<match>product</match>
		<item>
			<type>text</type>
			<label>Color Price</label>
			<selector>.product-container .price</selector>
			<attrs>color</attrs>
		</item> 
 		<item>
			<type>text</type>
			<label>Color Price Old</label>
			<selector>.product-container .old-price</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>BgColor Cart</label>
			<selector>.product-container .exclusive</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Color Cart</label>
			<selector>.product-container .exclusive</selector>
			<attrs>color</attrs>
		</item> 
	</items>
	<items>
		<header>Modules default in Sidebar</header>
		<match>modules</match>
			<item>
				<type>text</type>
				<label>Heading Background</label>
				<selector>.sidebar .block .title_block</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Heading Color</label>
				<selector>.sidebar .block .title_block a, .sidebar .block .title_block</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Content Background</label>
				<selector>.sidebar .block .block_content</selector>
				<attrs>background-color</attrs>
			</item>  

			<item>
				<type>text</type>
				<label>Content Color</label>
				<selector>.sidebar .block .block_content</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Border Color</label>
				<selector>.sidebar .block .block_content, .sidebar .block_content li </selector>
				<attrs>border-color</attrs>
			</item> 
	</items>


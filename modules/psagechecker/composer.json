{
    "name": "prestashop/psagechecker",
    "description": "Add a popup asking for age on certain page of your website",
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "prepend-autoloader": false
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "prestashop/php-coding-standards": "dev-master",
        "phpstan/phpstan": "^0.11.12"
    },
    "autoload": {
        "classmap": [
            "psagechecker.php",
            "controllers",
            "classes"
        ]
    }
}
